    keybd_event('T', 0, 2, 0); //松开  
    keybd_event('T', 0, 0, 0); //按下  
    keybd_event('T', 0, 2, 0); //松开   
    keybd_event('T', 0, 0, 0); //按下  
    keybd_event('T', 0, 2, 0); //松开  
    keybd_event('T', 0, 0, 0); //按下  
    keybd_event('T', 0, 2, 0); //松开  
}  
