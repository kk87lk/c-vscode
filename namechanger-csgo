#include<stdio.h>
#include<stdlib.h>
#include<Windows.h>
void click_set()  
{  
    //移到steam 
    SetCursorPos(26,19);  
    //鼠标单击  
    mouse_event(MOUSEEVENTF_LEFTDOWN, 0, 0, 0,0); //左键按下  
    mouse_event(MOUSEEVENTF_LEFTUP, 0, 0, 0, 0);  //左键松开
    sleep(20);
    //move to settings
    SetCursorPos(26,143);  
    //鼠标单击  
    mouse_event(MOUSEEVENTF_LEFTDOWN, 0, 0, 0,0); //左键按下  
    mouse_event(MOUSEEVENTF_LEFTUP, 0, 0, 0, 0);  //左键松开
    sleep(20);
    //click the name
    SetCursorPos(324,104);  
    //鼠标单击  
    mouse_event(MOUSEEVENTF_LEFTDOWN, 0, 0, 0,0); //左键按下  
    mouse_event(MOUSEEVENTF_LEFTUP, 0, 0, 0, 0);  //左键松开
    sleep(20);
    //全选delete
    keybd_event(17,0,0,0);
    keybd_event(65,0,0,0);
    keybd_event(65,0,2,0);
    keybd_event(17,0,2,0);
    keybd_event(8,0,0,0);
    keybd_event(8,0,2,0);
}
void click_confirm()  
{  
    //该函数把光标移到屏幕的指定位置  
    SetCursorPos(605,597);  
    //鼠标单击  
    mouse_event(MOUSEEVENTF_LEFTDOWN, 0, 0, 0,0); //左键按下  
    mouse_event(MOUSEEVENTF_LEFTUP, 0, 0, 0, 0);  //左键松开  
    sleep(20);
}
void name_NOOBS()  
{  
    //键盘输入  
    keybd_event('E', 0, 0, 0); //松开  
    keybd_event('E', 0, 2, 0); //按下  
    keybd_event('Z', 0, 0, 0); //按下  
    keybd_event('Z', 0, 2, 0); //松开        
    keybd_event('P', 0, 0, 0); //按下  
    keybd_event('P', 0, 2, 0); //松开  
    keybd_event('Z', 0, 0, 0); //按下  
    keybd_event('Z', 0, 2, 0); //松开    
    keybd_event('N', 0, 0, 0); //按下  
    keybd_event('N', 0, 2, 0); //松开        
    keybd_event('O', 0, 0, 0); //按下  
    keybd_event('O', 0, 2, 0); //松开  
    keybd_event('O', 0, 0, 0); //按下  
    keybd_event('O', 0, 2, 0); //松开   
    keybd_event('B', 0, 0, 0); //按下  
    keybd_event('B', 0, 2, 0); //松开  
    keybd_event('S', 0, 0, 0); //按下  
    keybd_event('S', 0, 2, 0); //松开 

}  
void name_AIMBOT()  
{  
