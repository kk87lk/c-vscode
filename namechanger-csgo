int main()  
{  
    click_set();
    name_NOOBS();
    click_confirm();

    click_set();
    name_Legitbot();
    click_confirm();

    click_set();
    name_AIMBOT();
    click_confirm();

    click_set();
    name_TRIGGERBOT();
    click_confirm();

    click_set();
    name_NOROCOIL();
    click_confirm();

    click_set();
    name_Ragebot();
    click_confirm();

    click_set();
    name_AIMWARE();
    click_confirm();
}  